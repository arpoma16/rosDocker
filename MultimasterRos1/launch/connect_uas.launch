<launch>
    <arg name="uav_id" default="2"/>
    <node name="master_discovery" pkg="fkie_master_discovery" type="master_discovery">
        <!-- todos los UAVS-->
        <rosparam param="robot_hosts">[ grvc-Xavier-NX,ubuntu,delta-pi,rock-4c-plus,nvidia,khadas,grvc-dell-miguel]</rosparam>
        <rosparam param="send_mcast">False</rosparam>
        <rosparam param="listen_mcast">False</rosparam>
    </node>
    <node name="master_sync" pkg="fkie_master_sync" type="master_sync">    
        <!-- todos los UAVS-->
        <rosparam param="sync_hosts">[grvc-Xavier-NX,ubuntu,delta-pi,rock-4c-plus,nvidia,khadas,grvc-dell-miguel]</rosparam> 
    </node>

    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch"/>
</launch>